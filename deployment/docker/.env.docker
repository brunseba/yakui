# Kubernetes Admin UI - Docker Development Configuration
VITE_API_BASE_URL=http://backend:3001/api
# Multicluster support - auto-detect available clusters
VITE_CLUSTER_CONTEXT=
NODE_ENV=development

# Backend Configuration
API_PORT=3001
API_TIMEOUT=30000
# Multicluster backend configuration
CLUSTER_CONTEXT=
ENABLE_MULTICLUSTER=true
KUBECONFIG_AUTO_DETECT=true

# Feature Flags
VITE_ENABLE_STUB_FEATURES=true
VITE_ENABLE_SERVER_MASKING=false
VITE_ENABLE_VERBOSE_LOGGING=true

# Docker-specific settings
CHOKIDAR_USEPOLLING=true
WATCHPACK_POLLING=true

# Optional: Enable debug logging
DEBUG=kubernetes-admin-ui:*
LOG_LEVEL=debug