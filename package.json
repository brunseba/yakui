{
  "name": "yakui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "dev:api": "node dev-server.cjs",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\" --kill-others-on-fail",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "health": "curl -s http://localhost:3001/api/health | jq",
    "version": "curl -s http://localhost:3001/api/version | jq",
    "status": "curl -s http://localhost:3001/api/health && echo && curl -s http://localhost:3001/api/version",
    "validate:config": "ts-node -e \"const { validateConfig, config } = require('./src/config/environment.ts'); const errors = validateConfig(config); if (errors.length > 0) { console.error('Config errors:', errors); process.exit(1); } else { console.log('âœ… Configuration valid'); }\"",
    "diagnostics": "node -e \"const { generateDiagnosticsReport } = require('./src/utils/systemValidation.ts'); generateDiagnosticsReport().then(report => console.log(report));\"",
    "kill:dev": "pkill -f 'dev-server.cjs' && pkill -f 'vite'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@kubernetes/client-node": "^1.3.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.3",
    "@tanstack/react-query": "^5.89.0",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "events": "^3.3.0",
    "globals": "^16.4.0",
    "https-browserify": "^1.0.0",
    "jsdom": "^27.0.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-node": "^10.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
