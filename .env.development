# Development Environment Configuration

# Kubernetes Proxy Server Configuration
VITE_PROXY_URL=http://localhost:3002
VITE_ENABLE_DIRECT_ACCESS=true

# Default cluster settings
VITE_K8S_DEFAULT_NAMESPACE=default

# Development features
VITE_ENABLE_CORS_WARNINGS=true
VITE_LOG_LEVEL=debug

# Frontend URLs (for CORS)
FRONTEND_URL=http://localhost:5173

# Kubernetes proxy server port (separate from API backend)
K8S_PROXY_PORT=3002
